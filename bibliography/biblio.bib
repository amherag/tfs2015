@article{Connor2005,
  abstract = {The aim of this study was to evaluate the effectiveness of using external indicators, such as commodity prices and currency exchange rates, in predictingmovements in the Dow Jones Industrial Average index. The performance of each technique is evaluated using different domain-specific metrics. A comprehensive evaluation procedure is described, involving the use of trading simulations to assess the practical value of predictive models, and comparison with simple benchmarks that respond to underlying market growth. In the experiments presented here, basing trading decisions on aneuralnetwork trained on a range of external indicators resulted in a return on investment of 23.5\% per annum, during a period when the DJIA index grew by 13.03\% per annum. A substantial dataset has been compiled and is available to other researchers interested in analysing financial time series. Celem tego badania była ocena skuteczno\'{s}ci korzystania z zewnętrznych wska\'{z}nik\'{o}w, takich jak cen surowc\'{o}w i kurs\'{o}w walut, w przewidywaniu ruch\'{o}w na indeksie Dow Jones przemysłowej \'{s}rednia. Wykonanie każdej z technik jest oceniana za pomocą r\'{o}żnych domenach okre\'{s}lonych wska\'{z}nik\'{o}w.Kompleksowa procedura oceny jest opisane, polegającego na użyciu symulacji handlowych, aby oceni\'{c} praktyczną warto\'{s}\'{c} modeli predykcyjnych i por\'{o}wnania z prostych wzorc\'{o}w, kt\'{o}re reagują na podstawowej wzrostu rynku. W badaniach przedstawionych tutaj, opierając decyzje handlowe na aneuralnetwork przeszkolona w zakresie zewnętrznych wska\'{z}nik\'{o}w spowodowało zwrot z inwestycji z 23,5\% w skali roku, w okresie, gdy indeks DJIA wzr\'{o}sł o 13.03\% w skali roku.Znaczny zbi\'{o}r danych został opracowany i jest dostępne dla innych badaczy w analizie finansowej szereg\'{o}w czasowych.},
  author = {Connor, Niall O and Madden, Michael G},
  file = {:Users/Amherag/Dropbox/papers/dcc-ace/00b7d5249d3c18eae2000000.pdf:pdf},
  mendeley-groups = {ArtificialTrading},
  title = {{A Neural Network Approach to Predicting Stock Exchange Movements using External Factors}},
  year = {2005},
}


@article{Li1999,
  abstract = {Recent studies in finance domain suggest that technical analysis may have merit to predictability of$\backslash$nstock. Technical rules are widely used for market assessment and timing. For example, moving average$\backslash$nrules are used to make buy or sell decisions at each day. In this paper, to explore the potential$\backslash$nprediction power of technical analysis, we present a genetic programming based system FGP (Financial$\backslash$nGenetic Programming), which specialises in taking some well known technical rules and adapting them$\backslash$nto prediction problems. FGP uses the power of genetic programming to generate decision trees through$\backslash$nefficient combination of technical rules with self-adjusted thresholds. The generated rules are more$\backslash$nsuitable for the prediction problem at hand. FGP was tested extensively on historical S\&P 500 data$\backslash$nthrough a specific prediction problem. Preliminary results show that it outperforms commonly used,$\backslash$nnon-adaptive, individual technical rules with respect to prediction accuracy and annualised rate of$\backslash$nreturn over two different out-of-sample test periods (three and a half year in each period).},
  author = {Li, Jin and Tsang, E.P.K.},
  file = {:Users/Amherag/Dropbox/papers/dcc-ace/FLAIRS99-019.pdf:pdf},
  journal = {Proceedings of The 12th International Florida AI Research Society Conference, Orlando, Florida},
  mendeley-groups = {ArtificialTrading},
  pages = {108--112},
  title = {{Improving technical analysis predictions: an application of genetic programming}},
  url = {http://scholar.google.com/scholar?hl=en\&btnG=Search\&q=intitle:Improving+Technical+Analysis+Predictions+:+An+Application+of+Genetic+Programming\#0},
  year = {1999},
}


@article{Lu2009,
  abstract = {As financial time series are inherently noisy and non-stationary, it is regarded as one of the most challenging applications of time series forecasting. Due to the advantages of generalization capability in obtaining a unique solution, support vector regression (SVR) has also been successfully applied in financial time series forecasting. In the modeling of financial time series using SVR, one of the key problems is the inherent high noise. Thus, detecting and removing the noise are important but difficult tasks when building an SVR forecasting model. To alleviate the influence of noise, a two-stage modeling approach using independent component analysis (ICA) and support vector regression is proposed in financial time series forecasting. ICA is a novel statistical signal processing technique that was originally proposed to find the latent source signals from observed mixture signals without having any prior knowledge of the mixing mechanism. The proposed approach first uses ICA to the forecasting variables for generating the independent components (ICs). After identifying and removing the ICs containing the noise, the rest of the ICs are then used to reconstruct the forecasting variables which contain less noise and served as the input variables of the SVR forecasting model. In order to evaluate the performance of the proposed approach, the Nikkei 225 opening index and TAIEX closing index are used as illustrative examples. Experimental results show that the proposed model outperforms the SVR model with non-filtered forecasting variables and a random walk model.},
  author = {Lu, Chi-Jie and Lee, Tian-Shyug and Chiu, Chih-Chou},
  doi = {10.1016/j.dss.2009.02.001},
  file = {:Users/Amherag/Dropbox/papers/dcc-ace/ICA+SVR\_DSS\_page.pdf:pdf},
  isbn = {01679236},
  issn = {01679236},
  journal = {Decision Support Systems},
  keywords = {financial time series forecasting,independent component analysis,support vector regression},
  mendeley-groups = {ArtificialTrading},
  number = {2},
  pages = {115--125},
  pmid = {206604507},
  publisher = {Elsevier B.V.},
  title = {{Financial time series forecasting using independent component analysis and support vector regression}},
  url = {http://dx.doi.org/10.1016/j.dss.2009.02.001},
  volume = {47},
  year = {2009},
}


@article{Garcia-Almanza2006,
  abstract = {In recent years the computers have shown to be a$\backslash$npowerful tool in financial forecasting. Many machine$\backslash$nlearning techniques have been used to predict movements$\backslash$nin financial markets. Machine learning classifiers$\backslash$ninvolve extending the past experiences into the future.$\backslash$nHowever the rareness of some events makes difficult to$\backslash$ncreate a model that detect them. For example bubbles$\backslash$nburst and crashes are rare cases, however their$\backslash$ndetection is crucial since they have a significant$\backslash$nimpact on the investment. One of the main problems for$\backslash$nany machine learning classifier is to deal with$\backslash$nunbalanced classes. Specifically Genetic Programming$\backslash$nhas limitation to deal with unbalanced environments. In$\backslash$na previous work we described the Repository Method, it$\backslash$nis a technique that analyses decision trees produced by$\backslash$nGenetic Programming to discover classification rules.$\backslash$nThe aim of that work was to forecast future$\backslash$nopportunities in financial stock markets on situations$\backslash$nwhere positive instances are rare. The objective is to$\backslash$nextract and collect different rules that classify the$\backslash$npositive cases. It lets model the rare instances in$\backslash$ndifferent ways, increasing the possibility of$\backslash$nidentifying similar cases in the future. The objective$\backslash$nof the present work is to find out the factors that$\backslash$nwork in favour of Repository Method, for that purpose a$\backslash$nseries of experiments was performed.},
  author = {Garcia-Almanza, Alma Lilia and Tsang, Edward P K},
  file = {:Users/Amherag/Dropbox/papers/dcc-ace/http\_\_\_repec.org\_sce2006\_up.13879.1141401469.pdf:pdf},
  journal = {12th International Conference On Computing In Economics And Finance},
  keywords = {genetic algorithms,genetic programming},
  mendeley-groups = {ArtificialTrading},
  pages = {number 489},
  title = {{Forecasting stock prices using Genetic Programming and Chance Discovery}},
  url = {http://ideas.repec.org/p/sce/scecfa/489.html},
  year = {2006},
}


