@article{Koza1992,
  abstract = {Many seemingly different problems in machine learning,$\backslash$nartificial intelligence, and symbolic processing can be$\backslash$nviewed as requiring the discovery of a computer program$\backslash$nthat produces some desired output for particular$\backslash$ninputs. When viewed in this way, the process of solving$\backslash$nthese problems becomes equivalent to searching a space$\backslash$nof possible computer programs for a highly fit$\backslash$nindividual computer program. The recently developed$\backslash$ngenetic programming paradigm described herein provides$\backslash$na way to search the space of possible computer programs$\backslash$nfor a highly fit individual computer program to solve$\backslash$n(or approximately solve) a surprising variety of$\backslash$ndifferent problems from different fields. In the$\backslash$ngenetic programming paradigm, populations of computer$\backslash$nprograms are genetically bred using the Darwinian$\backslash$nprinciple of survival of the fittest and using a$\backslash$ngenetic crossover (sexual recombination) operator$\backslash$nappropriate for genetically mating computer programs.$\backslash$nThis chapter shows how to reformulate seemingly$\backslash$ndifferent problems into a common form (i.e. a problem$\backslash$nrequiring discovery of a computer program) and, then,$\backslash$nto show how the genetic programming paradigm can serve$\backslash$nas a single, unified approach for solving problems$\backslash$nformulated in this common way.},
  author = {Koza, John R},
  file = {:Users/Amherag/Dropbox/papers/dcc-ace/Genetic Programming - John Koza.pdf:pdf},
  journal = {Dynamic, Genetic, and Chaotic Programming},
  keywords = {genetic programming,genetic algorithms},
  mendeley-groups = {ArtificialTrading},
  number = {June},
  pages = {203--321},
  title = {{The genetic programming paradigm: Genetically breeding populations of computer programs to solve problems}},
  url = {http://www.genetic-programming.com/jkpdf/soucek1992.pdf},
  year = {1992},
}


@book{Angeline1994,
  abstract = {Genetic programming may be more powerful than neural networks and other machine learning techniques, able to solve problems in a wider range of disciplines. In this ground-breaking book, John Koza shows how this remarkable paradigm works and provides substantial empirical evidence that solutions to a great variety of problems from many different fields can be found by genetically breeding populations of computer programs. Genetic Programming contains a great many worked examples and includes a sample computer code that will allow readers to run their own programs.In getting computers to solve problems without being explicitly programmed, Koza stresses two points: that seemingly different problems from a variety of fields can be reformulated as problems of program induction, and that the recently developed genetic programming paradigm provides a way to search the space of possible computer programs for a highly fit individual computer program to solve the problems of program induction. Good programs are found by evolving them in a computer against a fitness measure instead of by sitting down and writing them.John R. Koza is Consulting Associate Professor in the Computer Science Department at Stanford University.},
  author = {Angeline, Peter J.},
  booktitle = {Biosystems},
  doi = {10.1016/0303-2647(94)90062-0},
  file = {:Users/Amherag/Dropbox/papers/dcc-ace/MIT - Genetic Programming.pdf:pdf},
